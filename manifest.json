{
    "name": "DDelgado- BNIA Fast Stat",
    "version": "0.0.0.1",
    "description": "Baltimore City Indicator Lookup: quickly reference community statistics when browsing.",
    "manifest_version": 2,
    "permissions": [
        "https://observablehq.com/*"
    ],
    "background": {
        "scripts": [
            "./src/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "./images/icons/icon.png",
            "48": "./images/icons/icon.png"
        },
        "default_popup": "./src/pages/popup.html"
    },
    "icons": {
        "16": "./images/icons/icon.png",
        "48": "./images/icons/icon.png",
        "128": "./images/icons/icon.png"
    }
}